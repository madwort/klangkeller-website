<html>
<%
slotDurations = function(slots, maxLength){

console.log(slots);

	var longestSlot
	var options = ""
	var subtotal = 0

	slots.forEach( function (slot)
	{
	    var x = slot.duration
	    subtotal += +x
	    console.log(subtotal);

	});

	var timeleft = 121-subtotal

	if (timeleft === 1){
		longestSlot = 10
	} else {
		longestSlot = maxLength		
	}

	for (i = 1; i <= longestSlot/5; i++) {
		x = i*5
	    var option = '<option value="' + x + '">' + x + 'minutes</option>';
	    options += option
	} 

	return options
}
%>
</html>